{
  "name": "ui-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretty": "npx prettier . --write",
    "prepare-css": "find . -name '*.example.scss' -exec sh -c 'cp \"$0\" \"${0%.example.scss}.scss\"' {} \\;",
    "prepare-husky": "npx husky init",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@types/js-cookie": "^3.0.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.9",
    "immer": "^10.1.1",
    "js-cookie": "^3.0.5",
    "next": "14.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "react-tooltip": "^5.28.0",
    "tailwind-merge": "^2.5.4",
    "tsc": "^2.0.4",
    "yup": "^1.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0-rc.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.5.1"
}
